=begin cell

=object image: image-url="${thumbnail.high}", image-downloadable=yes, style=img_youtube

=begin title
=(object label: text="${title}", style=label_title)=
=end title

=begin published
=(object label: type=date, text="${published-at}", \
            source-format="yyyy-MM-dd'T'HH:mm:ss.SSSX", format="yyyy년 MM월 dd일", style=label_published)=
=end published

=object label: text="${duration}", style=label_duration

=end cell
