=begin header

=object image: filename=background_image_0001.jpg, alt="배경 이미지", style=img_bg
=object image: image-url="${itunes.image}", image-downloadable=yes, alt="커버", style=img_cover



=object button: action=pane, pane=P_PODCAST_LIST, target=object, object=P_001, label="${title}", alt="팟캐스트 이름", style=btn_podcast



=begin toolbar

=(object label: type=title, style=label_episode)=

